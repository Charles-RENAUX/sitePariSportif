rows IOException, JsonProcessingException {
		JsonNode node =3D jsonParser.getCodec().readTree(jsonParser);

		String value =3D node.asText();

		return constructor.apply(value);
	}

}

public class ValueObjectSerializer&lt;T&gt; extends StdSerializer&lt;T&gt; =
{

	private final Function&lt;T, String&gt; valueExtractor;

	public ValueObjectSerializer(Class vc, Function&lt;T, String&gt; valueExtr=
actor) {
		this.valueExtractor =3D valueExtractor;
	}

	@Override
